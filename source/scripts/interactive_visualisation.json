{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 400,
    "title": "Pro chess players in the world",
    "data": {
        "url": "https://raw.githubusercontent.com/TONG0006/FIT3179_assignment_2/data_transformation/source/datasets/transformed/country_stats.csv"
    },
    "transform": [
        {
            "calculate": "length(datum)",
            "as": "test"
        }
    ],
    "mark": "circle",
    "encoding": {
        "x": {
            "field": "test",
            "type": "quantitative",
            "title": "Confirmed Cases"
        },
        "y": {
            "field": "rating_standard",
            "type": "quantitative"
        },
        "color": {
            "field": "region",
            "type": "nominal"
        },
        "size": {
            "field": "rating_standard",
            "type": "quantitative"
        }
    }
}