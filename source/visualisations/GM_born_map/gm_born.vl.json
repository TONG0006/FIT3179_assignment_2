{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 500,
    "title": "Earthquakes of magnitude 4 or large between 4 Oct 2020 and 10 Oct 2020",
    "projection": {
        "type": "equalEarth"
    },
    "layer": [
        {
            "data": {
                "graticule": {
                    "step": [
                        15,
                        15
                    ]
                }
            },
            "mark": {
                "type": "geoshape",
                "stroke": "lightgrey",
                "opacity": 0.3
            }
        },
        {
            "data": {
                "url": "https://raw.githubusercontent.com/TONG0006/FIT3179_assignment_2/GM_birthplace/source/datasets/transformed/world_map_graticules.topojson",
                "format": {
                    "type": "topojson",
                    "feature": "custom.geo"
                }
            },
            "mark": {
                "type": "geoshape",
                "fill": "white",
                "stroke": "darkgrey"
            }
        },
        {
            "data": {
                "url": "https://raw.githubusercontent.com/TONG0006/FIT3179_assignment_2/GM_birthplace/source/datasets/transformed/gm_birthplace.csv"
            },
            "mark": {
                "type": "circle",
                "tooltip": {
                    "content": "data"
                }
            },
            "encoding": {
                "longitude": {
                    "field": "Longitude",
                    "type": "quantitative"
                },
                "latitude": {
                    "field": "Latitude",
                    "type": "quantitative"
                }
            }
        }
    ]
}